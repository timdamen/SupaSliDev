{
  "name": "@supaslidev/dashboard",
  "version": "0.1.0",
  "description": "Dashboard for managing supaSliDev presentations",
  "bin": {
    "supaslidev": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "scripts",
    "server"
  ],
  "type": "module",
  "main": "./dist/cli/index.js",
  "exports": {
    ".": {
      "import": "./dist/cli/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "dev:api": "node server/api.js",
    "build": "node scripts/generate-presentations.mjs && vite build && tsdown",
    "build:cli": "tsdown",
    "preview": "vite preview",
    "generate": "node scripts/generate-presentations.mjs",
    "typecheck": "vue-tsc --noEmit && tsc --noEmit -p tsconfig.cli.json",
    "lint": "oxlint --config ../../oxlint.json .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "tsdown": "^0.12.5",
    "tsx": "^4.19.0",
    "typescript": "^5.3.3",
    "vite": "^6.0.7",
    "vitest": "^3.0.0",
    "vue-tsc": "^2.0.0"
  }
}
