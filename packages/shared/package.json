{
  "name": "@supaslidev/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Shared components, styles, and utilities for supaslidev presentations",
  "exports": {
    ".": {
      "import": "./index.ts"
    },
    "./components/*": "./components/*",
    "./snippets/*": "./snippets/*",
    "./styles/*": "./styles/*",
    "./themes": {
      "import": "./themes/index.ts"
    },
    "./themes/*": "./themes/*"
  },
  "files": [
    "components",
    "snippets",
    "styles",
    "themes",
    "index.ts"
  ],
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vue-tsc": "^2.0.0"
  }
}
